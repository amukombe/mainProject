<% @page_title = "Subjects|List" %>
<h3>Listing subjects</h3>

<table class="table table-striped" style="font-size:14px;">
  <tr>
    <th>SUBJECT NAME</th>
    <th>SUBJECT LEVEL</th>
    <th>ACTION</th>
  </tr>

<% @subjects.each do |subject| %>
  <tr>
    <td><%= subject.subjectname %></td>
    <td><%= subject.subjectlevel %></td>
    <td><%= link_to 'Show', {:action=>'show', :id=>subject.id}, :class=>"btn-link"  %> |
        <%= link_to 'Edit', {:action=>'edit', :id=>subject.id}, :class=>"btn-link"  %> |
        <%= link_to 'Destroy', {:action=>'delete', :id=>subject.id}, :class=>"btn-link"  %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Subject', {:action=>'new'} , :class=>"btn btn-medium btn-info" %>
