<h3>Listing caricullums</h3>
<% @page_title = "caricullum" %>
<table class="table table-striped" style="font-size:14px;">
  <tr>
    <th>Level</th>
    <th>Action</th>
  </tr>

<% @caricullums.each do |caricullum| %>
  <tr><td>
        <%= caricullum.caricullumname %>
  </td>
    <td><%= link_to 'Show', caricullum, :class=>"btn-link" %> | 
    <%= link_to 'Edit', edit_caricullum_path(caricullum), :class=>"btn-link" %> |
    <%= link_to 'Destroy', caricullum, confirm: 'Are you sure?', method: :delete, :class=>"btn-link" %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Caricullum', new_caricullum_path, :class=>"btn btn-medium btn-info"  %>
